{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/auth/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api|auth).*)",
            "destination": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/gradbuddy",
            "destination": "/",
            "permanent": false
        }
    ]
}